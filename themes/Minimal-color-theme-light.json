{
  "name": "Not So Minimal Light",
  "type": "light",
  "colors": {
    "sideBar.background": "#f5f5f5",
    "sideBar.foreground": "#525252",
    "editor.foreground": "#111111",
    "editor.background": "#f5f5f5",
    "activityBarBadge.background": "#a8a8a8",
    "sideBarTitle.foreground": "#242424"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a1a1a1"
      }
    },
    {
      "name": "String, Regex",
      "scope": ["string"],
      "settings": {
        "foreground": "#1b64c4"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#ff4271"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#ff4271"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#ff4271"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#ff4271"
      }
    }
  ]
}
